{
  "name": "xpouch-ai-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "XPouch AI - A modern AI assistant with LangGraph backend",
  "scripts": {
    "dev": "concurrently \"pnpm --prefix frontend run dev\" \"cd backend && uv run main.py\"",
    "dev:frontend": "pnpm --prefix frontend run dev",
    "dev:backend": "cd backend && uv run main.py",
    "build": "pnpm --prefix frontend run build",
    "preview": "pnpm --prefix frontend run preview",
    "test": "pnpm --prefix frontend run test",
    "lint": "pnpm --prefix frontend run lint",
    "clean": "rm -rf frontend/node_modules backend/.venv frontend/dist backend/__pycache__"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  },
  "workspaces": [
    "frontend"
  ],
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.28.1"
}
